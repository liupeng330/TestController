@model IEnumerable<MvcApplication.Models.TaskGroup>

@{
    ViewBag.Title = "TaskGroup Index";
}

<h2>TaskGroup Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TaskGroupName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TaskGroupName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TaskGroupID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TaskGroupID })
            @Html.ActionLink("Schedule", "Schedule", "TaskSchedule", new {taskGroupID = item.TaskGroupID}, null)
        </td>
    </tr>
}

</table>
