<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="exposeExceptionDetail">
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
        <behavior name ="metadataBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://localhost:3721/jobservice/metadata"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="JobService.JobService" behaviorConfiguration="metadataBehavior">
        <endpoint address="http://localhost:3721/jobservice" binding="wsDualHttpBinding" contract="JobServiceInterface.IJobService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>