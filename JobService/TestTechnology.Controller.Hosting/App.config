<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="TestJobDBContext" connectionString="Data Source=192.168.1.108;Initial Catalog=TestJobDB;Persist Security Info=True;User ID=sa;Password=123;MultipleActiveResultSets=True"
        providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="exposeExceptionDetail">
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
        <behavior name ="metadataBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://localhost:3721/jobservice/metadata"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="TestTechnology.Controller.Service.JobService" behaviorConfiguration="exposeExceptionDetail">
        <endpoint address="http://localhost:3721/jobservice" binding="wsDualHttpBinding" contract="TestTechnology.Controller.Interface.IJobService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>